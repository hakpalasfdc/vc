    <validationRules>
        <fullName>Premier_WhenBenefitTypeIsCheckbox</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Benefit_Type__c , &apos;Checkbox&apos;) 

&amp;&amp; 

(Benefit_Checkbox__c = false

|| 

NOT(ISBLANK(Benefit_Amount__c))

|| 

NOT(ISBLANK(Benefit_Discount__c)))

&amp;&amp; 

NOT(OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;) 
)</errorConditionFormula>
        <errorMessage>You must check Benefit Checkbox. Benefit Amount and Benefit Discount is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Premier_WhenBenefitTypeIsCurrency</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Benefit_Type__c , &apos;Currency&apos;)

&amp;&amp; 

(ISBLANK(Benefit_Amount__c) 

||

(NOT(ISBLANK( Benefit_Discount__c)) 

||
 
Benefit_Checkbox__c = true))

&amp;&amp; 

NOT(OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;) 
)</errorConditionFormula>
        <errorMessage>You must enter a Benefit Amount. Benefit Discount and Benefit Checkbox is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Premier_WhenBenefitTypeIsDiscount</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Benefit_Type__c , &apos;Discount&apos;) 

&amp;&amp; 

(ISBLANK(Benefit_Discount__c) 

|| 

NOT(ISBLANK(Benefit_Amount__c)) 

|| 

Benefit_Checkbox__c = true)

&amp;&amp; 

NOT(OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;) 
)</errorConditionFormula>
        <errorMessage>You must enter a Benefit Discount. Benefit Amount and Benefit Checkbox is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR32_Active_Benefit_Validation</fullName>
        <active>true</active>
        <description>Only End Date can be changed on an Active Premier Benefit</description>
        <errorConditionFormula>ISPICKVAL(Status__c, &apos;Active&apos;) &amp;&amp;

OR(ISCHANGED(Name),
ISCHANGED(Benefit_Category__c),
ISCHANGED(Description__c),
ISCHANGED(Benefit_Type__c),
ISCHANGED(Benefit_Amount__c),
ISCHANGED(Benefit_Discount__c),
ISCHANGED(Benefit_Checkbox__c),
ISCHANGED(Effective_Date__c)
)
&amp;&amp;
NOT(OR( $Profile.Name = &quot;System Administrator&quot;,
								$Profile.Name = &quot;VSP Technical User&quot;))</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Only End Date can be changed on an Active Premier Benefit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR32_Effective_Date_Validation</fullName>
        <active>true</active>
        <description>Error when user tries to enter an Effective date in the past or today.</description>
        <errorConditionFormula>OR(ISNEW(),
ISCHANGED(Effective_Date__c))
&amp;&amp;
Effective_Date__c &lt;= Today()
&amp;&amp;
NOT(OR( $Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;))</errorConditionFormula>
        <errorDisplayField>Effective_Date__c</errorDisplayField>
        <errorMessage>You cannot enter an Effective Date as of today or in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR32_End_Date_Validation</fullName>
        <active>true</active>
        <description>Error when End Date is less than or equal to effective date</description>
        <errorConditionFormula>OR(ISNEW(),
ISCHANGED(End_Date__c))
&amp;&amp;
(End_Date__c &lt;=  Effective_Date__c ||  End_Date__c  &lt;=  TODAY() )
&amp;&amp;
NOT(OR( $Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;))</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>You cannot enter an End Date the same or earlier than Effective Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR32_Inactive_Benefit_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Status__c, &apos;Inactive&apos;) &amp;&amp; 

OR(ISCHANGED(Name), 
ISCHANGED(Benefit_Category__c),
ISCHANGED(Description__c), 
ISCHANGED(Benefit_Type__c), 
ISCHANGED(Benefit_Amount__c), 
ISCHANGED(Benefit_Discount__c), 
ISCHANGED(Benefit_Checkbox__c), 
ISCHANGED(Effective_Date__c), 
ISCHANGED(End_Date__c) 
) 
&amp;&amp; 
NOT(OR( $Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;))</errorConditionFormula>
        <errorMessage>You cannot make changes to an Inactive benefit</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
